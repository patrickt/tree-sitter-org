{
  "name": "org",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "line"
          }
        }
      ]
    },
    "line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "text": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": ".+"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "# "
          },
          {
            "type": "PATTERN",
            "value": ".*"
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

