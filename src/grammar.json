{
  "name": "org",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "meta"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "block"
          }
        }
      ]
    },
    "meta": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#+"
          },
          {
            "type": "FIELD",
            "name": "key",
            "content": {
              "type": "PATTERN",
              "value": "[^:]+"
            }
          },
          {
            "type": "STRING",
            "value": ": "
          },
          {
            "type": "FIELD",
            "name": "value",
            "content": {
              "type": "PATTERN",
              "value": ".+"
            }
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    },
    "block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "code"
        },
        {
          "type": "SYMBOL",
          "name": "line"
        }
      ]
    },
    "line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "code": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "#+BEGIN_SRC"
            },
            {
              "type": "STRING",
              "value": "#+begin_src"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "#+END_SRC"
            },
            {
              "type": "STRING",
              "value": "#+end_src"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "text": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": ".+"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "# "
          },
          {
            "type": "PATTERN",
            "value": ".*"
          },
          {
            "type": "STRING",
            "value": "\n"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [
    "text"
  ],
  "supertypes": []
}

